{
  "name": "domain-look-up-servers",
  "version": "1.0.0",
  "description": "A crawler for IANA root database to generate domain lookup servers",
  "type": "module",
  "scripts": {
    "crawl": "bun run src/crawler.ts",
    "dev": "bun run --watch src/crawler.ts",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/bun": "^1.3.2",
    "lefthook": "^2.0.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cheerio": "^1.1.2"
  }
}
